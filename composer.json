{
    "name": "cms-orbit/core",
    "version": "4.0.0-alpha.1",
    "description": "A powerful Laravel admin panel and resource management system",
    "keywords": [
        "laravel",
        "admin",
        "cms",
        "crud",
        "dashboard",
        "resources",
        "ui"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "CMS Orbit",
            "email": "ceo@amuz.co.kr"
        }
    ],
    "homepage": "https://github.com/cms-orbit/core",
    "support": {
        "issues": "https://github.com/cms-orbit/core/issues",
        "source": "https://github.com/cms-orbit/core"
    },
    "require": {
        "php": "^8.4",
        "composer-runtime-api": "^2.2",
        "composer/semver": "^3.0",
        "laravel/framework": "^10.0 || ^11.0 || ^12.0",
        "laravel/scout": "^9.0 || ^10.0",
        "spatie/laravel-translatable": "^6.8",
        "spatie/eloquent-sortable": "^4.4",
        "spatie/laravel-activitylog": "^4.9",
        "spatie/laravel-sluggable": "^3.7",
        "hulkur/laravel-hasmany-keyby": "^8.0",
        "ralphjsmit/laravel-seo": "^1.6",
        "tabuna/breadcrumbs": "^4.0",
        "twbs/bootstrap-icons": "^1.10",
        "watson/active": "^7.0",
        "ext-dom": "*"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.14",
        "orchestra/testbench": "^8.0 || ^9.0 || ^10.0",
        "orchestra/testbench-core": "^8.0 || ^9.0 || ^10.0",
        "orchestra/testbench-dusk": "^8.0 || ^9.0 || ^10.0",
        "phpunit/phpunit": "^10.0 || ^11.0 || ^12.0"
    },
    "conflict": {
        "laravel/framework": "<=10.48.25, <=11.34.0",
        "orchestra/testbench-core": "<8.23.9",
        "nesbot/carbon": "<2.66.0"
    },
    "autoload": {
        "psr-4": {
            "CmsOrbit\\Core\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "CmsOrbit\\Core\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CmsOrbit\\Core\\CoreServiceProvider"
            ],
                "aliases": {
                    "Alert": "CmsOrbit\\Core\\Support\\Facades\\Alert",
                    "Dashboard": "CmsOrbit\\Core\\Support\\Facades\\Dashboard",
                    "OrbitPackage": "CmsOrbit\\Core\\Support\\Facades\\OrbitPackage",
                    "EntityManager": "CmsOrbit\\Core\\Support\\Facades\\EntityManager"
                }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

